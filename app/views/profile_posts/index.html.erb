<p id="notice"><%= notice %></p>

<h1>Profile Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_posts.each do |profile_post| %>
      <tr>
        <td><%= link_to 'Show', profile_post %></td>
        <td><%= link_to 'Edit', edit_profile_post_path(profile_post) %></td>
        <td><%= link_to 'Destroy', profile_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile Post', new_profile_post_path %>
